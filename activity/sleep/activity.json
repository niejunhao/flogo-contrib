  "ref": "github.com/vijaynalawade/flogo-contrib/activity/sleep",
  "version": "0.0.1",
  "title": "Sleep Activity",
  "description": "Sleep for specified interval",
  "homepage": "https://github.com/vijaynalawade/flogo-contrib/activity/sleep",
